{
  "name": "cine_vertentes_webservice",
  "version": "1.1.0",
  "description": "API para consumo dos filmes em cartaz no Cine Plaza Barbacena e Cine Glória São João Del Rey.",
  "main": "src/index.js",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon -V",
    "lint": "./node_modules/.bin/eslint src/*",
    "production": "pm2 start server.js --name cine_plaza_webservice",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "npm test -- --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdmeida/spotify-wrapper.git"
  },
  "_moduleAliases": {
    "@controllers": "src/api/controllers",
    "@models": "src/api/models",
    "@schemas": "src/api/schemas",
    "@services": "src/api/services",
    "@src": "src",
    "@routes": "src/api/routes",
    "@utils": "src/api/utils"
  },
  "keywords": [
    "js",
    "node",
    "api"
  ],
  "author": "Wagner Almeida <wdmeida@gmail.com> (https://www.github.com/wdmeida)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wdmeida/spotify-wrapper/issues"
  },
  "homepage": "https://github.com/wdmeida/spotify-wrapper#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.1",
    "jsdom": "^11.6.2",
    "lodash": "^4.17.5",
    "module-alias": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.1",
    "nyc": "^11.5.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.2",
    "sinon-chai": "^3.0.0",
    "sinon-stub-promise": "^4.0.0"
  }
}
